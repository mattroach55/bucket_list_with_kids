`<%= simple_form_for(
  review, url: experience_reviews_path(type_review.destination, type_review, review),
  remote: true ) do |f| %>
  <%= f.input :description, as: :text, input_html: { id: "content-review"} %>
  <%= f.input :rating, :collection => %w[1 2 3 4 5], input_html: { id: "rating-review"} %>
  <%= f.submit "Save", class: "btn btn-primary" %>
<% end %>
